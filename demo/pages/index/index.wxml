<!--index.wxml-->
<view class="container">
    <!-- 第一个弹框 -->
    <van-popup show="{{ firShow }}" position="bottom" custom-style="max-height: 73%;" bind:close="onClose">
        <view class="title">
            <text bindtap="onClose"></text>
            <text>选择考试</text>
        </view>
        <view class="select {{firId == item.id ? 'selectActive' : ''}}" wx:for="{{ firstClassify }}" wx:key="index" data-id="{{item.id}}" data-title="{{item.title}}" bindtap="openSec">
            <view>{{ item.title }}</view>
            <view wx:if="{{item.brief}}">{{ item.brief }}</view>
        </view>
    </van-popup>
    <!-- 第二个弹框 -->
    <van-popup show="{{ secShow }}" position="bottom" custom-style="height: 30%;" bind:close="onClose">
        <view class="title">
            <text bindtap="backFir"></text>
            <text>{{ secTitle }}</text>
        </view>
        <view class="ks select {{ secId == 1 ? 'selectActive' : '' }}" data-id="1" bindtap="openLast">
            <view>在校生</view>
        </view>
        <view class="ks select {{ secId == 2 ? 'selectActive' : '' }}" data-id="2" bindtap="openLast">
            <view>在职</view>
        </view>
    </van-popup>
    <!-- 第三个弹框 -->
    <van-popup show="{{ lastShow }}" position="bottom" custom-style="max-height: 73%;" bind:close="onClose">
        <view class="title">
            <text bindtap="backSec"></text>
            <text>{{ secTitle }}</text>
            <text class="fulfil" bindtap="complete" wx:if="{{ LastId != null }}">完成</text>
        </view>
        <view class="ks select {{ LastId == item.major_id ? 'selectActive' : ''}}" data-id="{{ item.major_id }}" bindtap="selectLast" wx:for="{{ lastList }}" wx:key="index">
            <view>{{ item.major_name }}</view>
        </view>
    </van-popup>
    <view class="header">
        <view bindtap="openFirShow" data-id="{{navTitle.id}}">{{ navTitle.title }} > </view>
        <view>
            <image src="../../images/搜索.png"></image>
            <image src="../../images/小怪兽.png"></image>
        </view>
    </view>
    <view class="nav">
        <view class="{{navId == 1 ? 'active' : ''}}" data-id="{{1}}" bindtap="selectNav">精选</view>
        <view class="{{navId == item.id ? 'active' : ''}}" wx:for="{{navList}}" data-id="{{item.id}}" bindtap="selectNav" wx:key="index">
            {{item.title}}
        </view>
    </view>
    <view class="swiper">
        <swiper circular="true" style="height: 130px;" autoplay="true" interval="2000" indicator-dots="true" indicator-active-color="#fff">
            <swiper-item wx:for="{{banner}}" wx:key="index">
                <image src="{{item.banner_img}}"></image>
            </swiper-item>
        </swiper>
    </view>
    <view class="zb">
        <view class="left">
            <image src="https://wap.lexuemiao.com/img/haiyangyuyuye@3x.95e33040.png"></image>
            <image src="https://wap.lexuemiao.com/img/haiyangyuyuye3.36c87209.png"></image>
        </view>
        <view class="mid">
            |
        </view>
        <view class="right">
            <view class="time">
                19:00
            </view>
            <view class="name">
                23管综考前40天面授集训说明会（27日直播）
            </view>
        </view>
    </view>
    <view class="coursetime">
        <view class="pho">
            <image src="data:image/png;base64,"></image>
        </view>
        <view class="time">
            距离考研{{ config.residue_date }}天
        </view>
    </view>
    <view class="hone">
        <view class="hone-title">
            <view>{{ courseList.model_name }}</view>
            <view>
                查看全部 >
            </view>
        </view>
        <view class="home-jx-box">
            <view class="home-jx-item" wx:for="{{courseList.course_list}}" wx:key="index">
                <view class="home-jx-avatar">
                    <image src="/images/直播.png"></image>
                    <image  src="{{item.teacher[0].avatar}}"></image>
                </view>
                <view class="home-jx-title">
                    {{ item.title }}
                </view>
                <view class="home-jx-price">
                    免费
                </view>
                <view class="home-jx-tip">
                    {{ item.sales_num }}人已报名
                </view>
            </view>
        </view>
        <view class="hone-title">
            <view style="margin-top: 20rpx;">推荐好课</view>
        </view>
        <view class="lb-item" wx:for="{{9}}" wx:key="index">
            <view class="lb-title">
                23政英｜考研政英80+特训营
            </view>
            <view class="label-type">
                2022.07.07
            </view>
            <view class="lbi-teacher">
                <view>
                    <image src="https://lexuemiao.oss-cn-beijing.aliyuncs.com/uploads/image/2020af1zvps41607912321.jpg"></image>
                    <text>陈正康</text>
                </view>
                <view>
                    <image src="https://lexuemiao.oss-cn-beijing.aliyuncs.com/uploads/image/2020576ztzcv1579054635.jpg"></image>
                    <text>曲艺</text>
                </view>
            </view>
            <view class="lbi-info">
                <view>240人以报道</view>
                <view>免费</view>
            </view>
            <text class="lb-label">考验英语</text>
        </view>
    </view>
</view>