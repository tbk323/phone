.selectActive {
    background-color: #f15a24 !important;
    color: #fff;
}
.van-popup .select view:nth-child(1) {
    font-size: 28rpx;
    margin-top: 18rpx;
}
.van-popup .select view:nth-child(2) {
    font-size: 25rpx;
}

.ks {
    line-height: 60rpx;
}

.van-popup .select {
    width: 80%;
    margin: 20rpx auto;
    text-align: center;
    height: 100rpx;
    background-color: #f8f8f5;
    border-radius: 50rpx;
    overflow: hidden;
}

.van-popup .title text:nth-child(1) {
    display: inline-block;
    content: '';
    width: 20rpx;
    height: 20rpx;
    border-bottom: 2px solid #5b6476;
    border-left: 2px solid #5b6476;
    transform: rotate(45deg);
    position: absolute;
    left: -200rpx;
}
.van-popup .title text:nth-child(1) {
    position: relative;
}
.van-popup .title .fulfil {
    position: absolute;
    right: 70rpx;
    display: inline-block;
    width: 100rpx;
    height: 50rpx;
    line-height: 50rpx;
    background-color: #f15a24;
    color: #fff;
    border-radius: 30rpx;
    font-size: 25rpx;
}

.van-popup .title {
    text-align: center;
    margin: 30rpx 0;
}

.hone .lb-item .lbi-info view:nth-child(1) {
    color: #666666;
}
.hone .lb-item .lbi-info view:nth-child(2) {
    color: green;
    font-size: 28rpx;
}
.hone .lb-item .lbi-info {
    display: flex;
    justify-content: space-between;
    font-size: 25rpx;
    margin-top: 20rpx;
}

.hone .lb-item .lbi-teacher view text {
    margin-left: 10rpx;
}

.hone .lb-item .lbi-teacher view {
    display: flex;
    align-items: center;
}

.hone .lb-item .lbi-teacher {
    display: flex;
    color: #666666;
    font-size: 25rpx;
}

.hone .lb-item .label-type {
    font-size: 23rpx;
    margin: 20rpx 0;
    background-color: #f5f6f8;
    display: inline-block;
    padding: 5rpx 10rpx;
    box-sizing: border-box;
    border-radius: 20rpx;
}

.hone .lb-item .label-type text {
    background-color: #f4f4f4;
    width: 100rpx;
    height: 40rpx;
    display: inline-block;
    line-height: 40rpx;
    margin-right: 10rpx;
    text-align: center;
    color: #999999;
    border-radius: 5rpx;
}

.hone .lb-item .lb-title {
    margin-top: 30rpx;
    font-size: 28rpx;
}

.hone .lb-item .lb-label {
    position: absolute;
    left: 0;
    top: 0;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    width: 100rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 23rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #fff;
    text-align: center;
    overflow: hidden;
}

.hone .lb-item {
    background: #fff;
    box-shadow: 0 2px 2px 1px hsl(0deg 0% 81% / 35%);
    border-radius: 10rpx;
    padding: 20rpx;
    position: relative;
    margin-top: 50rpx;
}

.hone .lb-item image {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
}

.home-jx-box .home-jx-item .home-jx-tip {
    text-align: center;
    color: #f15a24;
}
.home-jx-box .home-jx-item .home-jx-price {
    width: 120rpx;
    height: 30rpx;
    line-height: 30rpx;
    border-radius: 0.53333rem;
    background: #f15a24;
    text-align: center;
    color: #fff;
    margin: 0.26667rem auto;
}

.home-jx-box .home-jx-item .home-jx-title {
    width: 90%;
    margin: 20rpx auto;
}

.home-jx-box .home-jx-item image {
    position: absolute;
    top: -35rpx;
    left: 70rpx;
}

.home-jx-box .home-jx-item image:nth-child(1) {
    width: 80rpx;
    height: 80rpx;
}
.home-jx-box .home-jx-item image:nth-child(2) {
    width: 75rpx;
    height: 75rpx;
    border-radius: 50%;
    margin-top: 2rpx;
    margin-left: 5rpx;
}

.home-jx-box .home-jx-item {
    position: relative;
    width: 31%;
    background-color: #eaeaea;
    border-radius: 10rpx;
    font-size: 20rpx;
    padding: 30rpx;
    box-sizing: border-box;
}
.home-jx-box {
    display: flex;
    justify-content: space-around;
    margin-top: 80rpx;
}

.hone .hone-title {
    color: #424963;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.hone .hone-title view:nth-child(1) {
    font-size: 35rpx;
    font-weight: 600;
}
.hone .hone-title view:nth-child(2) {
    font-size: 25rpx;
}
.hone {
    padding: 20rpx;
}

.coursetime {
    width: 90%;
    margin: 0 auto;
    background-color: #fafafa;
}

.coursetime .pho {
    text-align: center;
    margin: 20rpx 0;
}

.coursetime .pho image {
    width: 80rpx;
    height: 40rpx;
}

.coursetime .time {
    text-align: center;
    color: #a3aaba;
    font-size: 28rpx;
}

.zb {
    display: flex;
    width: 90%;
    padding: 10rpx;
    box-sizing: border-box;
    border-radius: 50rpx;
    margin: 10rpx auto;
    background-color: #fff;
    box-shadow: 0 0 0.53333rem 0 rgb(0 0 0 / 8%);
    align-items: center;
}

.zb .left {
    width: 30%;
}

.zb .mid {
    color: #eee;
    margin-right: 10rpx;
}

.zb .right {
    width: 60%;
    display: flex;
    align-items: center;
    font-size: 25rpx;
}

.zb .right .time {
    width: 18%;
    font-weight: 600;
}

.zb .right .name {
    width: 70%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.zb image:nth-child(1) {
    width: 50rpx;
    height: 50rpx;
}

.zb image:nth-child(2) {
    width: 120rpx;
    height: 35rpx;
}

.header {
    display: flex;
    justify-content: space-between;
    padding: 25rpx;
    box-sizing: border-box;
    color: #424963;
    font-weight: 600;
    font-size: 30rpx;
}

.header image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}

.header image:nth-child(1) {
    margin-right: 50rpx;
}

.nav {
    display: flex;
    justify-content: space-around;
    font-size: 26rpx;
    height: 50rpx;
}

.active {
    color: #f15a24;
    border-bottom: 2px solid #f15a24;
}

.swiper {
    text-align: center;
    padding: 20rpx;
    box-sizing: border-box;
}

.swiper image {
    border-radius: 20rpx;
    width: 95%;
    height: 100%;
}
